<div class="container-fluid">
<div class="row">
<div class="col-12">
  <h2 class='text-center'>Login</h2>
</div>
</div>

<div class='row'>

  <% if @survey_questions.any? %>
    <div class="col-12">
        <%= render :partial => 'survey_questions_form' %>
    </div>
  <% end %>

  <% login_form_partials.sort.reverse.each do |login_form| %>
    <div class="col-12">
      <%= render :partial => login_form %>
    </div>
  <% end %>
  
</div>
</div>
